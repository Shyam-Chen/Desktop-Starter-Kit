{
  "name": "Electron-Chromium-Starter",
  "version": "1.0.0",
  "description": "A boilerplate for Electron, Material, Parcel, Babel, PostHTML, and PostCSS.",
  "main": "main.js",
  "scripts": {
    "start": "concurrently --kill-others \"parcel src/index.html -d www\" \"electron .\"",
    "build": "parcel build src/index.html --public-url /www && electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Electron-Chromium-Starter.git"
  },
  "keywords": [
    "Electron"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Electron-Chromium-Starter/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Electron-Chromium-Starter#readme",
  "dependencies": {
    "material-components-web": "0.31.0"
  },
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "babel-preset-env": "1.6.1",
    "concurrently": "3.5.1",
    "cssnano": "3.10.0",
    "electron": "^1.8.2",
    "electron-builder": "20.2.0",
    "eslint": "4.18.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.9.0",
    "htmlhint": "0.9.13",
    "parcel-bundler": "1.6.2",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "11.1.0",
    "postcss-nested": "3.0.0",
    "posthtml-include": "1.1.0",
    "rucksack-css": "1.0.2",
    "stylelint": "9.1.1",
    "stylelint-config-standard": "18.1.0"
  }
}
