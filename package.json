{
  "name": "Electron-Desktop-Starter",
  "version": "1.0.0",
  "description": "A boilerplate for Electron, Material, Parcel, Babel, PostHTML, and PostCSS.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "electron": "electron",
    "build-windows": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Web GO\"",
    "build-windows:64": "electron-packager . --overwrite --asar=true --platform=win32 --arch=x64 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Web GO\"",
    "build-macos": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "build-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "lint": "gulp lint",
    "clean": "rimraf dist",
    "reset": "git reset --hard HEAD && git clean -df",
    "reinstall": "yarn cache clean && git clean -dfx -e '.*' && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Electron-Desktop-Starter.git"
  },
  "keywords": [
    "Electron"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Electron-Desktop-Starter/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Electron-Desktop-Starter#readme",
  "dependencies": {
    "material-components-web": "0.12.1"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-function-bind": "6.22.0",
    "babel-plugin-transform-imports": "1.2.0",
    "babel-preset-latest": "6.24.1",
    "browser-sync": "2.18.12",
    "caniuse-db": "1.0.30000672",
    "connect-history-api-fallback": "1.3.0",
    "cssnano": "3.10.0",
    "electron": "^1.6.10",
    "electron-packager": "^8.7.0",
    "electron-winstaller": "^2.5.2",
    "express": "4.15.3",
    "express-history-api-fallback": "2.2.1",
    "firebase-tools": "3.9.0",
    "gulp": "3.9.1",
    "gulp-eslint": "3.0.1",
    "gulp-htmlhint": "0.3.1",
    "gulp-htmlmin": "3.0.0",
    "gulp-inject": "4.2.0",
    "gulp-notify": "3.0.0",
    "gulp-plumber": "1.1.0",
    "gulp-protractor": "4.1.0",
    "gulp-rev": "7.1.2",
    "gulp-sitemap": "4.2.0",
    "gulp-sourcemaps": "2.6.0",
    "gulp-stylelint": "3.9.0",
    "gulp-template": "4.0.0",
    "gulp-util": "3.0.8",
    "jasmine-core": "2.6.2",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "1.7.0",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.3",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-prerollup-plugin": "1.0.1",
    "karma-rollup-plugin": "0.2.4",
    "phantomjs-prebuilt": "2.1.14",
    "postcss-conditionals": "2.1.0",
    "postcss-cssnext": "2.11.0",
    "postcss-define-function": "0.1.1",
    "postcss-each": "0.10.0",
    "postcss-extend": "1.0.5",
    "postcss-for": "2.1.1",
    "postcss-import": "10.0.0",
    "postcss-modules": "0.8.0",
    "postcss-scss": "1.0.0",
    "postcss-simple-vars": "4.0.0",
    "postcss-url": "6.1.0",
    "protractor": "5.1.2",
    "require-dir": "0.3.2",
    "rimraf": "2.6.1",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-buble": "0.15.0",
    "rollup-plugin-commonjs": "8.0.2",
    "rollup-plugin-html": "0.2.1",
