    "rollup-stream": "1.19.0",
    "rucksack-css": "0.9.1",
    "run-sequence": "1.2.2",
    "stylelint-config-standard": "16.0.0",
    "stylelint-scss": "1.4.4",
    "sw-precache": "5.1.1",
    "sw-toolbox": "3.6.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0"
  }
}
